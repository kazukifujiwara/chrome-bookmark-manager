{
    "manifest_version": 3,
    "name": "Custom Bookmark Manager",
    "version": "1.0",
    "description": "A modern new tab page with bookmark management.",
    "permissions": [
        "tabs",
        "favicon",
        "storage"
    ],
    "chrome_url_overrides": {
        "newtab": "index.html"
    }
}