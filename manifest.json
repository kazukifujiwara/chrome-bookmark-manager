{
    "manifest_version": 3,
    "name": "Custom Bookmark Manager",
    "version": "1.2",
    "description": "A modern new tab page with bookmark management.",
    "author": "Kazuki Fujiwara",
    "permissions": [
        "tabs",
        "favicon",
        "storage"
    ],
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}